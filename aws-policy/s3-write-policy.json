{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "GeneralAccountAccess",
            "Effect": "Allow",
            "Action": [
                "s3:ListAllMyBuckets",
                "s3:CreateBucket"
            ],
            "Resource": "*"
        },
        {
            "Sid": "AllowFirehoseWriteAndBucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:AbortMultipartUpload",
                "s3:ListMultipartUploadParts",
                "s3:GetBucketLocation",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::jm-demo-s3-lab",
                "arn:aws:s3:::jm-demo-s3-lab/*"
            ]
        }
    ]
}
